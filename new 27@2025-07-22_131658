apiVersion:apps/v1
kind: replicaSet
metadata:
   name:app
   label: app
spec:
   containers: nginx
   image name: nginx_iamge
   replica:3
   port:80
   
   kubectl 
   
   provider "aws" {
   region: "ap-south-1"
   }
   
   resource "aws_vpc" "example"{
   cidr = 10.0.0.0/16
   dns_support= true
   tag={
   name = "vpc"
   }
   }
   
   resource "aws_subnet" " eg1"{
   availability_zone= "ap-south-1a"
   cidr = 10.0.0.0/17
   map= aws_vpc.}
   
   aws_subnet, internet_gateway,aws_route_table,aws_security_group,
   
   terraform init
   terraform validate
   terraform plan, terraform apply
   
   